  "description": "A module package to manipulate several Rock Band game files.",
  "homepage": "https://github.com/ruggeryiury/rbtools#readme",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Ruggery Iury CorrÃªa <ruggeryiury@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "keywords": [
    "tool",
