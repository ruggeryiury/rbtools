    "javascript",
    "typescript",
    "python"
  ],
  "engines": {
    "node": ">=22.17.0"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "crc": "^4.3.2",
    "dotenv": "^17.2.3",
    "file-type": "^20.5.0",
    "jschardet": "^3.1.4",
    "node-lib": "link:..\\node-lib",
    "png-to-ico": "^2.1.8",
    "set-default-options": "link:..\\set-default-options",
    "sharp": "^0.34.4",
    "tempy": "^3.1.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.8.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.38.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.1.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.8",
    "tsx": "^4.20.6",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "scripts": {
    "build": "tsdown && copyfiles -u 1 src/bin/**/* dist",
    "clear": "rimraf dist",
    "dev": "tsx watch dev/main.ts",
    "format": "prettier --write src",
    "lint": "eslint src"
